ModIDS = {
    2470580497,
    2658586765,
}
--//
admins = {
    466460995,
}
--//
ver = {
    274293901,
}
--//
function emojis()
	for _,Player in pairs(game:GetService('Players'):GetChildren()) do
		if table.find(ModIDS, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚úîÔ∏èÔ∏è]' ..Player.DisplayName)
				end
			end
		elseif
			table.find(ver, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('REAL VER BTW')
				end
			end
		elseif
			table.find(admin, Player.UserId) then
			if Player.Character then
				if Player.Character.Parent.Name == 'Players' then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[üëì]' .. Player.DisplayName)
				end
			end
		if
			Player.Character then
			if Player.Character.Parent.Name == 'Players' then
				if not Player.Character.UpperTorso:FindFirstChild('OriginalSize') then
					Player.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[‚ù§Ô∏è]' .. Player.DisplayName)
				end
			end
		end
	end
	end
	end
return ModIDS
